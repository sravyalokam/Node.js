{
  "name": "prisma_poc",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "jest --watch",
    
    "config:banking": "node prisma/banking/prisma.banking.config.ts",
    "config:user": "node node prisma/user/prisma.user.config.ts",
    
    "prisma:banking:generate": "npm run config:banking && prisma generate",
    "prisma:user:generate": "npm run config:user && prisma generate",
    "prisma:generate:all": "npm run prisma:banking:generate && npm run prisma:user:generate",
    
    "migrate:banking": "npm run config:banking && prisma migrate dev",
    "migrate:user": "npm run config:user && prisma migrate dev",
    
    "seed:banking": "node prisma/banking/seed.js",
    "seed:user": "node prisma/user/seed.js",
    "seed:all": "npm run seed:banking && npm run seed:user",
    
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "start": "cross-env NODE_ENV=production node index.js",
    "build": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "cross-env": "^10.1.0",
    "jest": "^30.2.0",
    "prisma": "^7.1.0",
    "supertest": "^7.1.4",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "nodemon": "^3.1.11",
    "pg": "^8.16.3",
    "winston": "^3.19.0"
  }
}
