generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Customer {
  id    Int     @id @default(autoincrement())
  name  String
  email String  @unique
  phone String?
}

model Branch {
  id         Int    @id @default(autoincrement())
  branchName String
  city       String
}

model Employee {
  id     Int     @id @default(autoincrement())
  name   String
  age    Int
  salary Float
}